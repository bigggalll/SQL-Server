  </DataSources>
  <DataSets>
    <DataSet Name="HourlyRowCountsDetails">
      <Query>
        <DataSourceName>DynamicsPerf</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Statstime">
            <Value>=Parameters!Statstime.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Database">
            <Value>=Parameters!Database.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT *
FROM   PERF_HOURLY_ROWDATA_VW
WHERE  cast(STATS_TIME as varchar(20)) = @Statstime
and DATABASE_NAME =@Database
AND TABLE_NAME &lt;&gt; 'NULL'
and ROWS_DELTA &lt;&gt;0
order by rows_delta desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="STATS_TIME">
          <DataField>STATS_TIME</DataField>

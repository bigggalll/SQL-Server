  <DataSets>
    <DataSet Name="LongRunningQueries">
      <Query>
        <DataSourceName>DynamicsPerf</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Ticket">
            <Value>=Parameters!Ticket.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Status">
            <Value>=Parameters!Status.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT SQL_TEXT,
       TOTAL_ELAPSED_TIME,
       AVG_ELAPSED_TIME,
       AVG_LOGICAL_READS,
       EXECUTION_COUNT,
       B.COMMENT,
       TICKET_NUM,
       STATUS
FROM   COMMENTS B
       INNER loop JOIN (SELECT V.QUERY_HASH,
                               MAX(DATE)               AS DATE,
                               MAX(SQL_TEXT)           AS SQL_TEXT,
                               MAX(TOTAL_ELAPSED_TIME) AS TOTAL_ELAPSED_TIME,
                               MAX(AVG_ELAPSED_TIME)   AS AVG_ELAPSED_TIME,
                               MAX(AVG_LOGICAL_READS)  AS AVG_LOGICAL_READS,
                               MAX(EXECUTION_COUNT)    AS EXECUTION_COUNT
                        FROM   COMMENTS B
                               INNER JOIN QUERY_HISTORY_VW V
                                       ON B.QUERY_HASH = V.QUERY_HASH
                        WHERE  FLAG = 'M'
                        GROUP  BY V.QUERY_HASH,
                                  DATE) A
                    ON A.QUERY_HASH = B.QUERY_HASH
WHERE  B.COMMENT IS NOT NULL
       AND b.query_hash &lt;&gt; 0x000000000
AND TICKET_NUM in (@Ticket) and STATUS in (@Status)
ORDER  BY TOTAL_ELAPSED_TIME DESC </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="SQL_TEXT">
          <DataField>SQL_TEXT</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TOTAL_ELAPSED_TIME">
          <DataField>TOTAL_ELAPSED_TIME</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="AVG_ELAPSED_TIME">
          <DataField>AVG_ELAPSED_TIME</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="AVG_LOGICAL_READS">
          <DataField>AVG_LOGICAL_READS</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="EXECUTION_COUNT">
          <DataField>EXECUTION_COUNT</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="COMMENT">
          <DataField>COMMENT</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ticket_num">
          <DataField>TICKET_NUM</DataField>

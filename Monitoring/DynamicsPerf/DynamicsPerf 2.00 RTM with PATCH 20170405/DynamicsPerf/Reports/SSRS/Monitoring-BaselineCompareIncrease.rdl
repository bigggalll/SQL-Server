<?xml version="1.0" encoding="utf-8"?>
<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="Dynperf2008">
      <DataSourceReference>DynamicsPerf</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>2e508452-e644-4cfc-9aea-6b89d6956cbb</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Comparison">
      <Query>
        <DataSourceName>Dynperf2008</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Flag">
            <Value>=Parameters!Flag.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT TOP 100 A.SERVER_NAME,
               A.DATABASE_NAME,
               'QUERY_HASH'=master.dbo.fn_varbintohexstr(A.QUERY_HASH),
	'BEFORE_PLAN_HASH'=master.dbo.fn_varbintohexstr(A.BEFORE_PLAN_HASH),
	'AFTER_PLAN_HASH'=master.dbo.fn_varbintohexstr(A.AFTER_PLAN_HASH),
               A.EXECUTION_COUNT_TODAY AS EXECUTION_COUNT,
               A.BEFORE_AVG_TIME,
               A.CURRENT_AVG_TIME,
               A.[TIME_DIFF(ms)],
               A.[%DECREASE],
               (SELECT TOP 1 QUERY_PLAN_PARSED FROM QUERY_HISTORY_VW QHV WHERE QHV.SERVER_NAME = A.SERVER_NAME AND QHV.DATABASE_NAME = A.DATABASE_NAME
				AND QHV.QUERY_HASH = A.QUERY_HASH AND QHV.QUERY_PLAN_HASH = A.BEFORE_PLAN_HASH AND FLAG = 'M' ORDER BY DATE DESC) AS BEFORE_PARSED_PLAN,
				(SELECT TOP 1 QUERY_PLAN_PARSED FROM QUERY_HISTORY_VW QHV WHERE QHV.SERVER_NAME = A.SERVER_NAME AND QHV.DATABASE_NAME = A.DATABASE_NAME
				AND QHV.QUERY_HASH = A.QUERY_HASH AND QHV.QUERY_PLAN_HASH = A.AFTER_PLAN_HASH AND FLAG = 'M' ORDER BY DATE DESC) AS AFTER_PARSED_PLAN,
               B.QUERY_PLAN AS BEFORE_PLAN,
               C.QUERY_PLAN AS AFTER_PLAN
FROM   (SELECT DISTINCT STARTING.SERVER_NAME,
                        STARTING.DATABASE_NAME,
                        STARTING.QUERY_HASH,
						STARTING.QUERY_PLAN_HASH,
                        STARTING.EXECUTION_COUNT_TODAY,
                        STARTING.AVG_TIME_TODAY_MS  AS BEFORE_AVG_TIME,
                        ENDING.AVG_TIME_TODAY_MS    AS CURRENT_AVG_TIME,
                        ENDING.AVG_TIME_TODAY_MS - STARTING.AVG_TIME_TODAY_MS AS 'TIME_DIFF(ms)',
                        CAST(( ENDING.AVG_TIME_TODAY_MS - STARTING.AVG_TIME_TODAY_MS ) / CASE STARTING.AVG_TIME_TODAY_MS WHEN 0 THEN 1 ELSE STARTING.AVG_TIME_TODAY_MS END * 100 AS DECIMAL(14, 3)) AS '%DECREASE',
                       -- STARTING.SQL_TEXT,
                        STARTING.QUERY_PLAN_HASH AS BEFORE_PLAN_HASH,
                        ENDING.QUERY_PLAN_HASH   AS AFTER_PLAN_HASH
        FROM   QUERY_HISTORY STARTING
               INNER JOIN QUERY_HISTORY ENDING
                       ON STARTING.QUERY_HASH = ENDING.QUERY_HASH
                          AND STARTING.DATABASE_NAME = ENDING.DATABASE_NAME
                          AND ENDING.SERVER_NAME = STARTING.SERVER_NAME
        WHERE  STARTING.DATE = @StartDate
               AND STARTING.FLAG = @Flag
               AND ENDING.DATE =@EndDate
               AND ENDING.FLAG = @Flag
               AND STARTING.AVG_TIME_TODAY_MS &lt; ENDING.AVG_TIME_TODAY_MS
               AND STARTING.QUERY_HASH &lt;&gt; 0x0000000000000000) AS A
       CROSS APPLY (SELECT TOP 1 QUERY_PLAN
                    FROM   QUERY_PLANS W1
                    WHERE  W1.QUERY_PLAN_HASH = A.BEFORE_PLAN_HASH
                           AND W1.SERVER_NAME = A.SERVER_NAME
                           AND W1.DATABASE_NAME = A.DATABASE_NAME) AS B
       CROSS APPLY (SELECT TOP 1 QUERY_PLAN
                    FROM   QUERY_PLANS W2
                    WHERE  W2.QUERY_PLAN_HASH = A.AFTER_PLAN_HASH
                           AND W2.SERVER_NAME = A.SERVER_NAME
                           AND W2.DATABASE_NAME = A.DATABASE_NAME) AS C
		ORDER  BY 8 DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>

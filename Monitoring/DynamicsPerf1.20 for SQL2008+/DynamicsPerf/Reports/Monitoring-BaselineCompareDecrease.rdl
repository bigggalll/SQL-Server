        </Field>
        <Field Name="ID_DECREASE">
          <DataField>%DECREASE</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="SQL_TEXT">
          <DataField>SQL_TEXT</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BEFORE_PLAN">
          <DataField>BEFORE_PLAN</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AFTER_PLAN">
          <DataField>AFTER_PLAN</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Dynperf2008</DataSourceName>
        <CommandText>SELECT 'QUERY_HASH'=master.dbo.fn_varbintohexstr(A.QUERY_HASH),
	A.EXECUTION_COUNT,
       A.BEFORE_AVG_TIME,
       A.CURRENT_AVG_TIME,
       A.[TIME_DIFF(ms)],
       A.[%DECREASE],
       A.SQL_TEXT,
		'BEFORE_PLAN'=master.dbo.fn_varbintohexstr(B.QUERY_PLAN_HASH),
		'AFTER_PLAN'=master.dbo.fn_varbintohexstr(C.QUERY_PLAN_HASH)
FROM   (SELECT DISTINCT V1.QUERY_HASH,V1.EXECUTION_COUNT,
                        V1.AVG_ELAPSED_TIME                                                               AS BEFORE_AVG_TIME,
                        V2.AVG_ELAPSED_TIME                                                               AS CURRENT_AVG_TIME,
                        V2.AVG_ELAPSED_TIME - V1.AVG_ELAPSED_TIME                                         AS 'TIME_DIFF(ms)',
                        Cast(( V2.AVG_ELAPSED_TIME - V1.AVG_ELAPSED_TIME ) / CASE V1.AVG_ELAPSED_TIME
                                                                               WHEN 0 THEN 1
                                                                               ELSE V1.AVG_ELAPSED_TIME
                                                                             END * 100 AS DECIMAL(10, 3)) AS '%DECREASE',
                        V1.SQL_TEXT,
                        V1.QUERY_PLAN_HASH                                                                AS BEFORE_PLAN_HASH,
                        V2.QUERY_PLAN_HASH                                                                AS AFTER_PLAN_HASH
        FROM   QUERY_STATS_HASH_VW V1
               INNER JOIN QUERY_STATS_HASH_VW V2
                       ON V1.QUERY_HASH = V2.QUERY_HASH
where V1.RUN_NAME = @Baseline
AND V2.RUN_NAME = @Comparison
               AND V1.AVG_ELAPSED_TIME &lt; V2.AVG_ELAPSED_TIME
               AND V1.QUERY_HASH &lt;&gt; 0x0000000000000000) AS A
       CROSS APPLY (SELECT TOP 1 QUERY_PLAN_HASH
                    FROM   QUERY_PLANS W1
                    WHERE  W1.QUERY_PLAN_HASH = A.BEFORE_PLAN_HASH) AS B
       CROSS APPLY (SELECT TOP 1 QUERY_PLAN_hash
                    FROM   QUERY_PLANS W2
                    WHERE  W2.QUERY_PLAN_HASH = A.AFTER_PLAN_HASH) AS C
ORDER  BY 5 DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Baseline">
            <Value>=Parameters!Baseline.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Comparison">
            <Value>=Parameters!Comparison.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="Baseline">
      <Fields>
        <Field Name="run_name">
          <DataField>RUN_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Dynperf2008</DataSourceName>
        <CommandText>select  RUN_NAME from STATS_COLLECTION_SUMMARY
where RUN_NAME like 'BASE%'
order by RUN_NAME desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="BaselineOffset">
      <Fields>
        <Field Name="run_name">
          <DataField>RUN_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Dynperf2008</DataSourceName>
        <CommandText>SELECT        RUN_NAME
FROM            STATS_COLLECTION_SUMMARY
ORDER BY RUN_NAME DESC</CommandText>
      </Query>
    </DataSet>
  </DataSets>

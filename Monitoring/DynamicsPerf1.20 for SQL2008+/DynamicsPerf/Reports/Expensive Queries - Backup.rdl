      <Query>
        <DataSourceName>DynamicsPerf</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT TOP 100 DATABASE_NAME,EXECUTION_COUNT,
TOTAL_ELAPSED_TIME,SQL_TEXT,
QUERY_PLAN,TOTAL_LOGICAL_READS
FROM   QUERY_STATS_CURR_VW
where DATABASE_NAME in (@Database)
ORDER  BY TOTAL_ELAPSED_TIME DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@Database">
            <Value>=Parameters!Database.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
